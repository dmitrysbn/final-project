
<h2>Browse by Neighbourhood:</h2>

<div class="container">
  <div class="row">
  <% @neighbourhoods.each do |nbhd| %>
      <div class="col-md-3">
        <div class="nbhd-image">
          <%= link_to(neighbourhood_url(nbhd)) do %>
            <%= image_tag(nbhd.nbhd_image.url, border: 0, class: "img-responsive") %>
          <% end %>
        </div>
      </div>
  <% end %>
  </div>
</div>
